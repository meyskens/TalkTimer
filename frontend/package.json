    "file-loader": "^2.0.0",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "surge": "^0.20.1",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.3.3",
